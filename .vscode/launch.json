    "version": "0.2.0",
    "configurations": [
        {
            "name": "pocketsync_flutter",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "pocketsync_flutter (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "pocketsync_flutter (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "example",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define-from-file=example/dart-defines.json"
            ]
        },
        {
            "name": "example (profile mode)",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--dart-define-from-file=example/dart-defines.json"
            ]
        },
        {
            "name": "example (release mode)",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--dart-define-from-file=example/dart-defines.json"
            ]
        }
    ]
}